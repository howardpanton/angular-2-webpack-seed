{
  "name": "angular-2-webpack-seed",
  "version": "1.0.0",
  "description": "A webpack starter for angular 2",
  "scripts": {
    "start": "npm run clean_node && concurrently \"npm run sass\" \"npm run webpack\" ",
    "sass": "gulp build-dev --env=development",
    "webpack": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build_aot": "gulp build-prod --env=production",
    "build_webpack": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "aot": "node_modules/.bin/ngc -p ./src/tsconfig.aot.json",
    "rollup": "node_modules/.bin/rollup -c rollup.js"
  },
  "repository": {
    "type": "mercurial",
    "url": "https://github.com/howardpanton/angular-2-webpack-seed"
  },
  "author": "Howard Panton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/howardpanton/angular-2-webpack-seed"
  },
